#!/usr/bin/python3
from sys import argv

def is_prime(n):
    if n <= 1:
        return False
    elif n <= 3:
        return True
    elif n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True

def find_prime_factors(num):
    factors = []
    divisor = 2
    while num != 1:
        if is_prime(num):
            factors.append(num)
            break
        if num % divisor == 0:
            factors.append(divisor)
            num //= divisor
        else:
            divisor += 1
    return factors

with open(argv[1]) as f:
    for line in f:
        num = int(line.strip())
        factors = find_prime_factors(num)
        if len(factors) == 2:
            print("{}={}*{}".format(num, factors[0], factors[1]))
        else:
            print("Unable to find prime factors for {}".format(num))
